{
    "env": {
        "jest": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
    "ignorePatterns": [
        "web-build/*"
    ],
    "globals": {
        "__CLIENT__": true,
        "__SERVER__": true,
        "__DEV__": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "import",
        "jsx-a11y",
        "react"
    ],
    "root": true,
    "rules": {
    }
}
